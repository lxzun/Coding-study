   "source": [
    "def solution(answers):\n",
    "    answer = [0, 0, 0]\n",
    "    one = [1, 2, 3, 4, 5]\n",
    "    two = [2, 1, 2, 3, 2, 4, 2, 5]\n",
    "    three = [3, 3, 1, 1, 2, 2, 4, 4, 5, 5]\n",
    "\n",
    "    for i, x in enumerate(answers):\n",
    "        if one[i%5] == x:\n",
    "            answer[0]+=1\n",
    "        if two[i%8] == x:\n",
    "            answer[1]+=1\n",
    "        if three[i%10] == x:\n",
    "            answer[2]+=1\n",
    "\n",
    "    ans = []\n",
    "    M = max(answer)\n",
    "    if M == 0:\n",
    "        return ans\n",
    "\n",
    "    for i, v in enumerate(answer, 1):\n",
    "        if v == M:\n",
    "            ans.append(i)\n",
    "    return ans"
   ]
  },
