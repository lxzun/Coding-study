    "import math\n",
    "\n",
    "def solution(brown, yellow):\n",
    "    answer = []\n",
    "    a = 1\n",
    "    b = (brown+4)/2\n",
    "    c = brown+yellow\n",
    "    answer.append(int((b+math.sqrt(b**2-4*a*c))/2))\n",
    "    answer.append(int((b-math.sqrt(b**2-4*a*c))/2))\n",
    "    answer.sort(reverse=True)\n",
    "    return answer"
   ]
